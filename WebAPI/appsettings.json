{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Presence": {
    "TtlSeconds": 60,
    "HeartbeatSeconds": 30,
    "KeyPrefix": "sg",
    "GraceSeconds": 5,
    "MaxBatchSize": 200,
    "DefaultPageSize": 100,
    "MaxPageSize": 500
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Billing": {
    "EventCreationFeeCents": 50000,
    "MaxEventEscrowTopUpAmountCents": 50000000,
    "MaxWalletTopUpAmountCents": 20000000
  }
}
